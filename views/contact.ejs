    <label for="onderwerp">Onderwerp:</label>
    <textarea id="onderwerp" name="onderwerp" placeholder="Schrijf hier uw boodschap.." style="height:200px"></textarea>

    <input class="terug" type="button" value="Verzenden">

  </form>
</div>
